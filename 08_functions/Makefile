CC=clang
CXX=clang++
CXXFLAGS=-std=c++11
RM=rm -f
CPPFLAGS=-g
TARGETS=function functionprototype passbyvalue passbypointer passarray passarraypointer
TARGETS2=argstomain defaultargs
TARGETS3=functiontemplate functionoverload functiontemplatenontype functiontemplatetrailingreturntype functiontemplatetrailingreturntype2 pointerstofunctions recursivefunction

all: $(TARGETS) $(TARGETS2) $(TARGETS3)

clean:
	$(RM) $(TARGETS) $(TARGETS2) $(TARGETS3)
